{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "name": "sparkam-ai-v2-7",
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/upload",
      "destination": "/upload.html"
    },
    {
      "source": "/fan-link",
      "destination": "/fan-link.html"
    },
    {
      "source": "/how-it-works",
      "destination": "/how-it-works.html"
    },
    {
      "source": "/waitlist",
      "destination": "/waitlist.html"
    },
    {
      "source": "/admin-tools",
      "destination": "/admin-tools.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
